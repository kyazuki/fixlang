/*
https://wiki.haskell.org/99_questions/1_to_10

Problem 2
(*) Find the last but one element of a list.
*/

module Main;

find_last_but : Iterator a -> Option a;
find_last_but = |iter| (
    let len = iter.get_length;
    if len <= 1 { none() };
    iter.take(len-1).find_last
);

main : IO ();
main = (
    let iter = Iterator::from_array([1,2,3,4]);
    let last_but = iter.find_last_but;
    println(last_but.as_some.to_string)
);