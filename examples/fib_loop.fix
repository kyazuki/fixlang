module Main;

main : IO ();
main = (
    let arr = Array::fill(31, 0);
    let arr = arr.set!(0, 0);
    let arr = arr.set!(1, 1);
    let arr = loop((2, arr), |(idx, arr)|
        if idx == arr.get_size {
            break $ arr
        } else {
            let x = arr.get(idx-1);
            let y = arr.get(idx-2);
            let arr = arr.set!(idx, x+y);
            continue $ (idx+1, arr)
        }
    );
    println $ arr.get(30).to_string // 832040
);