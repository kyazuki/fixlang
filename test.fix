module Main;
import Debug;

main : IO ();
main = (
    eval assert_eq(|_|"", "".strip_first_spaces, "");
    eval assert_eq(|_|"", "Hello".strip_first_spaces, "Hello");
    eval assert_eq(|_|"", " Hello".strip_first_spaces, "Hello");
    eval assert_eq(|_|"", " \tHello".strip_first_spaces, "Hello");
    eval assert_eq(|_|"", " ".strip_first_spaces, "");
    eval assert_eq(|_|"", "  ".strip_first_spaces, "");

    pure()
);