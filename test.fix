module Main;
import Random;
import Debug;

main : IO ();
main = (
    let init = [/* 0x12345 = */ 74565_U64, /* 0x23456 = */ 144470_U64, /* 0x34567 = */ 214375_U64, /* 0x45678 = */ 284280_U64];
    let random = init_by_array(init);
    let n = 100000000;
    let (random, time) = consumed_time_while_lazy(|_| loop(
        (random, 0), |(random, i)|
        if i >= n {
            break $ random
        };
        let (x, random) = generate_U64(random);
        continue $ (random, i + 1)
    ));
    time.to_string.println
);